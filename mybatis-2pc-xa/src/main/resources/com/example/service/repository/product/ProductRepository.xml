<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.service.repository.product.ProductRepository">
	<select id="findProducts" resultType="Product">
		SELECT * FROM PRODUCT
	</select>
	<insert id="createProduct">
		INSERT INTO PRODUCT VALUES (#{code},#{name})
	</insert>
</mapper>